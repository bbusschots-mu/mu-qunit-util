{
    "source": "./src",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": { "access": ["public", "protected", "private"] },
                "autoPrivate": true,
                "unexportedIdentifier": {"enable": true},
                "manual": {
                    "files": [
                        "./docs-manual/defaultDummyData.md"
                    ]
                }
            }
        },
        {"name": "esdoc-node"}
    ]
}